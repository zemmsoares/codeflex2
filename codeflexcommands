**studentcode.pl** (on docker)

    :- initialization main.

    main :-
      current_prolog_flag(argv, Argv),
      format('Hello World, argv:~w\n', [Argv]),
      halt(0).

:-style_check(-singleton).

size([],0).
size([H|T],N) :- size(T,N1), N is N1+1,

print(N).

// done
swipl -s 002.pl -g "size([1,2,an,test],N)" -t halt. 2> stderr.txt > output.txt

